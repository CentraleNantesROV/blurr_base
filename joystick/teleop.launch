<launch>
 <!-- Turtlesim Node-->
  <node pkg="turtlesim" type="turtlesim_node" name="sim"/>

 <!-- joy node -->
 <group ns="turtle1">
  <node type="joy_node" name="turtle_joy"  pkg="joy" respawn="true">
    <param name="dev" type="string" value="/dev/input/js1" />
    <param name="deadzone" value="0.12" />
  </node>

 <!-- Axes to motion -->
  <node pkg="ecn_bluerov_base" type="joy_example.py" name="teleop"  output="screen" respawn="true"/>
  </group>
</launch>
